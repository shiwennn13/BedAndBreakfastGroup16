
@{
    ViewData["Title"] = "regSubscription";
}


<section class="testimonial-section spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="section-title">
                    <h2>Successful Notice</h2>
                    <span>Congratulation! You are now Subscribing our Newsletter!</span>
                    
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-8 offset-lg-2">
                <div class="testimonial-slider owl-carousel">
                    <div class="ts-item">
                        <p>For confirmation, please also check your email and click on confirm subscribe button to continue.</p>
                        <p>Subscription confirmation ID: @ViewBag.recordid</p>
                        <div class="form-group text-center">
                            
                            <a asp-action="Index" asp-controller="Home" class="btn btn-lg primary-btn" style="color:#333;">Back To Home &rarr;</a>
                        </div>

                        
                    </div>
                    
                </div>
            </div>
        </div>
    </div>
</section>



