
@{
    ViewData["Title"] = "send2Queue";
}

<section class="testimonial-section spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="section-title">
                    <h2>Booking Done</h2>
                    <span>Thank you for booking with us!</span>

                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-8 offset-lg-2">
                <div class="testimonial-slider owl-carousel">
                    <div class="ts-item">
                        <p>We have received your booking! Our crew will contact you as soon as possible. 
                            If you did not receive the email after 1 day of your booking, please contact us with the below information:</p>
                        <p>Your Booking ID: @ViewBag.BookingId</p>
                        <p>Your Transaction ID: @ViewBag.transactionID</p>
                        <p>Transaction Current Time: @DateTime.Now</p>
                        <div class="form-group text-center">

                            <a asp-action="Index" asp-controller="Home" class="btn btn-lg primary-btn" style="color:#333;">Back To Home &rarr;</a>
                        </div>


                    </div>

                </div>
            </div>
        </div>
    </div>
</section>

